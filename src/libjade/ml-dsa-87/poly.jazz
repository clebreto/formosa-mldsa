export fn JASMIN_MLDSA87_poly_add(reg ptr u32[N] rpc, reg ptr u32[N] rpa, reg ptr u32[N] rpb) -> reg ptr u32[N] {
  rpa = rpa; rpb = rpb; rpc = rpc;
  rpc = __poly_add(rpc, rpa, rpb);
  rpc = rpc;
  return rpc;
}

// Refactoring to discuss with B.G.
export fn JASMIN_MLDSA87_poly_add_inplace(reg u32 pc, reg u32 pa, reg u32 pb) {
  stack u32[N] a b;
  reg ptr u32[N] rpa rpb; 
  a = __mem_to_poly(pa);
  b = __mem_to_poly(pb);
  rpa = a; rpb = b;
  a = __poly_add_inplace(rpa, rpb);
  __poly_to_mem(a, pc);
}

export fn JASMIN_MLDSA87_poly_chknorm(reg ptr u32[N] rpa, reg u32 B)  -> reg u32 {
  rpa = rpa;  
