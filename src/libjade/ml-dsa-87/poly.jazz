export fn JASMIN_MLDSA87_poly_decompose(reg ptr u32[N] rpa1, reg ptr u32[N] rpa0, reg ptr u32[N] rpa) -> reg ptr u32[N], reg ptr u32[N] {
  rpa1 = rpa1; rpa0 = rpa0; rpa = rpa;

  rpa1, rpa0 = _poly_decompose(rpa1, rpa0, rpa);
  rpa1 = rpa1; rpa0 = rpa0;
  return rpa1, rpa0;
}

